AT_SETUP([Method])
AT_KEYWORDS([cond method])
PT_CHECK([ListenHTTP
	Service
		Method "GET"
		Backend
			Address
			Port
		End
	End
	Service
		Method "HEAD"
		Backend
			Address
			Port
		End
	End
End	
],
[GET /echo/foo
end

200
x-backend-number: 0
end

HEAD /echo/foo
end

200
x-backend-number: 1
end
])
